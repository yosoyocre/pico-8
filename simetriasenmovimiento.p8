pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
-- variables globales 
paletas = {
 	-- azules
	{1,7,12,13},
	-- verdes
	{3,11,10}, 
	-- azules y verdes
	{1,7,12,3,11,10},
	-- rojos
	{8,8,9,10},
	-- multicolor
	{8,9,10,11,12,13,14,15},
}

-- generan una coordenadas random 
-- para el cuarto cuadrante,
-- que es donde nos vamos a mover
function xrand()
	return 1+rnd(64)
end

function yrand()
	return 64+rnd(64)
end

-- genera velocidades random
function vrand(v)
	return min(1, max(-1, v + rnd(1) -0.5))
end

function calcularp(p, v, mi, ma)
	p2 = max(min(p + v, ma), mi)

	return p2
end

function calculax(x, v)
	return calcularp(x, v, 0, 64)
end

function calculay(y, v)
	return calcularp(y, v, 64, 128)
end

-- selecciona una paleta de colores
-- al azar
function escogerpaleta()
	return paletas[flr(rnd(#paletas))+1]
end

-- escoge un color al azar
-- de la paleta dada
function escogercolor(paleta)
	local p = flr(rnd(#paleta))+1
	return paleta[p]
end

-- modifica el array de figuras
-- añadiendo y eliminando elementos
-- aleatoriamente
function nuevo(borrartodo)
	local n = rnd(5)
	local rango1 = rnd(100)
	local rango2 = rnd(100)
	local nborrar = rnd(5)
	local b

	if (borrartodo) then 
		figuras = {}
		paleta = escogerpaleta()
	else
		if (nborrar < #figuras) then
			for i=0,nborrar do
				b = flr(rnd(#figuras))
				del(figuras,figuras[b])
			end
		end
	end

	if (rango1 > rango2) then
		local aux
		aux = rango2
		rango2 = rango1
		rango1 = aux
	end

	for i=0,n do
		local forma = rnd(100)
		local f
		-- círculo
		if (forma < rango1) then
			f = {
				t = 'circfill',
				x0 = xrand(),
				y0 = yrand(),
				r = rnd(4),
				c = escogercolor(paleta),
				vx0 = 0,
				vy0 = 0,
			}
		end
		-- línea
		if (forma >= rango1 and forma < rango2) then
			f = {
				t = 'line',
				x0 = xrand(),
				y0 = yrand(),
				x1 = xrand(),
				y1 = yrand(),
				c = escogercolor(paleta),
				vx0 = 0,
				vy0 = 0,
				vx1 = 0,
				vy1 = 0,
			}
		end
		-- rectángulo
		if (forma >= rango2) then
			f = {
				t = 'rect',
				x0 = xrand(),
				y0 = yrand(),
				x1 = xrand(),
				y1 = yrand(),
				c = escogercolor(paleta),
				vx0 = 0,
				vy0 = 0,
				vx1 = 0,
				vy1 = 0,
			}
		end

		add(figuras, f)
	end
end

-- dibuja las figuras del array
function dibujar(x,y)
	for f in all(figuras) do
		if (f.t == 'circfill') then
			circfill(f.x0, f.y0, f.r, f.c)			
		end
		if (f.t == 'line') then
			line(f.x0, f.y0, f.x1, f.y1, f.c)			
		end
		if (f.t == 'rect') then
			rect(f.x0, f.y0, f.x1, f.y1, f.c)			
		end
	end	
end

-- modifica ligeramente la posición
-- de las figuras
function mover()
	local nf
	for i,f in pairs(figuras) do
		nf = f
		if (nf.t == 'circfill') then
			nf.x0 = calculax(nf.x0, nf.vx0)
			nf.y0 = calculay(nf.y0, nf.vy0)
			nf.vx0 = vrand(nf.vx0)
			nf.vy0 = vrand(nf.vy0)
		end
		if (nf.t == 'line') then
			nf.x0 = calculax(nf.x0, nf.vx0)
			nf.y0 = calculay(nf.y0, nf.vy0)
			nf.x1 = calculax(nf.x1, nf.vx1)
			nf.y1 = calculay(nf.y1, nf.vy1)
			nf.vx0 = vrand(nf.vx0)
			nf.vy0 = vrand(nf.vy0)
			nf.vx1 = vrand(nf.vx1)
			nf.vy1 = vrand(nf.vy1)
		end
		if (nf.t == 'rect') then
			nf.x0 = calculax(nf.x0, nf.vx0)
			nf.y0 = calculay(nf.y0, nf.vy0)
			nf.x1 = calculax(nf.x1, nf.vx1)
			nf.y1 = calculay(nf.y1, nf.vy1)
			nf.vx0 = vrand(nf.vx0)
			nf.vy0 = vrand(nf.vy0)
			nf.vx1 = vrand(nf.vx1)
			nf.vy1 = vrand(nf.vy1)
		end

		figuras[i] = nf
	end	
end

-- crea imágenes en espejo de lo dibujado
-- en el cuarto cuadrante
function proyectar()
	for x=0,128 do
		for y=0,128 do
			local ang = atan2(64-x,64-y)
			if (ang > 0.125 and ang <= 0.375) or (ang > 0.625 and ang <= 0.875) then
				pset(x,y,pget(64-abs(64-y),64+abs(64-x)))
			end
			if (ang > 0.375 and ang <= 0.625) or (ang > 0.875) then
				pset(x,y,pget(64-abs(64-x),64+abs(64-y)))
			end			
		end
	end
end

function _init()
	t = 0
	figuras = {}
	paleta = escogerpaleta()
	nuevo()
end

function _update()
	if (t == 1000) then
		nuevo(true)
		t = 0
	else 
		if (t%100 == 0) then
			nuevo()
		end
	end
	t+=1
	mover()
end

function _draw()
	cls()
	dibujar()
	proyectar()
end
